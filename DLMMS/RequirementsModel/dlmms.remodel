	}

	Service ThirdPartyServices {

	}

	Contract UpdateModelAccessPermissionService::updatePermission(permission : String) : Boolean {

		definition:
			model:Model = Model.allInstance()->any(m:Model | m.AccessPermission = permission)

		precondition:
			true
		postcondition:
			true
	}

